{
  "name": "repro",
  "author": "Neil Chaudhuri",
  "description": "Issue 16252",
  "version": "3.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest --coverage",
    "test:basic": "jest",
    "typecheck": "tsc --p tsconfig.eslint.json --skipLibCheck --sourceRoot src --noEmit",
    "lint": "eslint src --ext=ts,tsx",
    "package": "npm run clean && npm run build && cpy .npmignore dist && npm pack"
  },
  "prettier": {
    "printWidth": 130,
    "tabWidth": 4,
    "useTabs": true,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": false,
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "parser": "typescript"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "\\.svg": "<rootDir>/__mocks__/svgrMock.tsx"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/build",
      "<rootDir>/dist"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.{jsx,tsx,ts}",
      "!**/__mocks__/*.*",
      "!**/src/custom.d.ts",
      "!**/src/index.ts"
    ],
    "setupFiles": [
      "<rootDir>/src/utils/__test__/setup-test.js",
      "<rootDir>/src/utils/__test__/testUtils.ts"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.9",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@types/cleave.js": "^1.4.4",
    "cleave.js": "^1.6.0",
    "date-fns-timezone": "^0.1.4",
    "date-fns-tz": "^1.1.6",
    "downshift": "^6.1.7",
    "framer-motion": "^4.1.17",
    "np": "^7.5.0",
    "react": "^17.0.2",
    "react-datepicker": "^4.2.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.0.3",
    "react-icons": "^4.2.0",
    "react-virtual": "^2.8.1",
    "regenerator-runtime": "^0.13.9",
    "timezones-list": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@reach/router": "^1.3.4",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-a11y": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/react": "^6.3.9",
    "@storybook/testing-react": "0.0.22",
    "@svgr/core": "^5.5.0",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.0",
    "@types/node": "^16.7.1",
    "@types/react": "^17.0.24",
    "@types/react-datepicker": "^4.1.4",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@vitejs/plugin-react": "^1.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "cpy": "^8.1.2",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jsdom": "^16.6.0",
    "react-hook-form": "^6.15.1",
    "react-test-renderer": "^17.0.2",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.0",
    "storybook-builder-vite": "0.0.12",
    "tslib": "^2.3.0",
    "typescript": "^4.4.2",
    "vite": "^2.6.1",
    "vite-plugin-react-svg": "^0.2.0",
    "vite-plugin-svgr": "^0.3.0",
    "vite-svg-loader": "^2.1.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/components.umd.js",
  "module": "./dist/components.es.js",
  "exports": {
    ".": {
      "import": "./dist/components.es.js",
      "require": "./dist/components.umd.js"
    }
  }
}
